{
  "name": "webpack-testing",
  "version": "0.0.3",
  "description": "test webpack functionality",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --config .eslintrc.js ",
    "build:watch": "npm i && webpack --progress --color --watch --display-error-details",
    "serve": "lite-server -c=bs-config.json",
    "build:prod": "webpack",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "mocha-webpack test/usersModule.test.js",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immnk/webpack-testing.git"
  },
  "keywords": [
    "webpack",
    "module"
  ],
  "author": "Mani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/immnk/webpack-testing/issues"
  },
  "homepage": "https://github.com/immnk/webpack-testing#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.2",
    "lite-server": "^2.4.0",
    "nyc": "^13.1.0"
  }
}
